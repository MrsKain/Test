@{
    Layout = null;
}
@using System.Data;
<!DOCTYPE html>
<html lang="zh-CN" class="bgc-fa">
<head>
<meta charset="UTF-8">
<meta name="keywords" content="">
<meta name="copyright" content="" />
<meta name="description" content="">
<meta name="viewport" content="width=device-width,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" />
<title>易考网</title>
<!-- css -->
<link rel="stylesheet" type="text/css" href="/Areas/Areas/Content/css/common.css"/>
<link rel="stylesheet" type="text/css" href="/Areas/Areas/Content/css/main.css"/>
<!-- js -->
<script type="text/javascript" src="/Areas/Areas/Content/js/jquery-1.11.3.min.js"></script>
<script type="text/javascript" src="/Areas/Areas/Content/js/index.js"></script>
</head>
<body class="bgc-fa">

  <!-- 头部 -->
      @{
        Html.RenderAction("Head", "Home", new { id = 2});
         }
  <!-- 头部 END -->

  <!-- 考试列表 -->
  <article class="exam-lists">
      @{
        Html.RenderAction("LeftMenu", "Home");
         }
    <ol id="tab-ol-1">
      <!-- 小学 -->
      <li class="teaches-li">
      
       
          @{
              DataTable dt = ViewBag.table as DataTable;
              foreach(DataRow dr in dt.Rows)
              {
                 <a class="primary-school"  href="/Areas/video/Videoinfor/@dr["id"].ToString()">
          <img  src="/Content/UploadFiles/images/@dr["images"].ToString()" alt="教师资格">
          <p class="p2">@dr["title"].ToString()</p>
        </a>
              }
              }

        <div class="clear"></div>
      </li>
    
    
    
    </ol>
    <div class="clear"></div>
  </article>
  <!-- 考试列表 END -->

</body>
</html>
<script src="/Content/js/jquery-2.0.3.min.js" type="text/javascript"></script>
<script type="text/javascript">
    function search(id)
    {
    
        location.href = "/Areas/Video/Videolist/"+id;
    }
    $(document).ready(function () {
       
        $("#tab-ul-1").children().each(function () {
           
            if ('@ViewBag.id' == $(this).attr("name"))
            {
                $(this).addClass("active");
            }

        })
    });
</script>